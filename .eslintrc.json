{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi" : "error",
        "prefer-const" : "error",
        "require-await" : "error",
        "no-console": "off"
    }
}
